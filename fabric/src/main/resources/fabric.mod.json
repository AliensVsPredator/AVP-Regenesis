{
  "schemaVersion": 1,
  "id": "avpr",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "${mod_url}",
    "issues": "${mod_issues}",
    "sources": "${mod_sources}"
  },
  "license": "${mod_license}",
  "icon": "${mod_logo}",
  "environment": "*",
  "entrypoints": {
    "main": [
      "org.avpr.fabric.FabricLibMod"
    ],
    "client": [
      "org.avpr.fabric.FabricClientMod"
    ]
  },
  "mixins": [
    "${mod_id}.fabric.mixins.json"
  ],
  "accessWidener": "${mod_id}.accesswidener",
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": "*",
    "java": ">=${java_version}",
    "azurelib": "~${azurelib_version}",
    "minecraft": "~${minecraft_version}"
  }
}